{
    "title": "Garlic Beef",
    "meta": "Serve with egg noodles",
    "ingredients": [
        { "text": "2 TBSP dry sherry", "type": "imperishable" },
        { "text": "1 lb filet mignon", "type": "meat" },
        { "text": "1 green bell pepper sliced", "type": "produce" },
        { "text": "4 garlic cloves diced", "type": "produce" },
        { "text": "6 scallions sliced", "type": "produce" },
        { "text": "4 TBSP soy sauce", "type": "imperishable" },
        { "text": "2 TBSP vegetable oil", "type": "imperishable" },
        { "text": "2 TBSP sesame seeds", "type": "imperishable" }
    ],
    "directions": [
        { "text": "Toast the sesame seeds in a wok on medium heat for 2 mins, set aside." },
        { "text": "Cut meat into thin strips." },
        { "text": "Add oil to wok on medium heat and fry beef for 3 mins." },
        { "text": "Add bell peppers and garlic and cook for 2 more mins." },
        { "text": "Add sherry, soy sauce, and scallions and cook for 1 more min." },
        { "text": "When serving sprinkle sesame seeds on top." }
    ]
}
