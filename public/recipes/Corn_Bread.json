{
    "title": "Corn Bread",
    "meta": "Grandma's",
    "ingredients": [
        { "text": "1 cup yellow corn meal", "type": "imperishable" },
        { "text": "1 cup flour", "type": "imperishable" },
        { "text": "&frac34; cup sugar", "type": "imperishable" },
        { "text": "4 tsp baking powder", "type": "imperishable" },
        { "text": "&frac12; tsp salt", "type": "spice" },
        { "text": "1 egg", "type": "imperishable" },
        { "text": "1 cup milk", "type": "perishable" },
        { "text": "&frac14 cup shortening", "type": "imperishable" }
    ],
    "directions": [
        { "text": "Sift together all dry ingredients." },
        { "text": "Mix in wet ingredients until smooth." },
        { "text": "Bake in a greased 9″x9″ pan at 425° for 22&frac12; mins." }
    ]
}
