{
    "title": "Venison Barbacos",
    "meta": "",
    "ingredients": [
        "3 lbs venison",
        "4 chipotles in adobo",
        "1 red onion",
        "5 garlic cloves",
        "&frac12; cup lime juice",
        "&frac12; cup vinegar",
        "1 quart beef stock",
        "&frac14; cup lard",
        "2 bay leaves",
        "1 tsp smoked paprika",
        "1 tsp cumin",
        "1 tsp ground cloves",
        "1 TBSP salt"
    ],
    "directions": [
        { "text": "Chop up onion and garlic." },
        { "text": "Put everything in a covered dutch oven on high and cook for 3 hours." },
        { "text": "Pull all the meat from bone and shred with forks, pour some of mixture over meat." }
    ]
}
