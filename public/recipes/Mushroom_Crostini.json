{
    "title": "Mushroom Crostini",
    "meta": "",
    "ingredients": {
        "Crostini": [
            { "text": "1 french baguette (cut into &frac12;″ slices)", "type": "perishable" },
            { "text": "&frac12; cup olive oil", "type": "imperishable" },
            { "text": "2 TBSP herbes de provence", "type": "spice" }
        ],
        "Topping": [
            { "text": "1 lb mushrooms (chopped at &frac12;″ pieces)", "type": "produce" },
            { "text": "1 tsp fresh thyme", "type": "produce" },
            { "text": "1 tsp fresh rosemary (chopped)", "type": "produce" },
            { "text": "&frac14; cup parsley (chopped)", "type": "produce" },
            { "text": "&frac14; cup olive oil", "type": "imperishable" },
            { "text": "1 TBSP worcestershire sauce", "type": "imperishable" },
            { "text": "a pinch of salt", "type": "spice" },
            { "text": "a pinch of black pepper", "type": "spice" },
            { "text": "6 oz goat cheese or mozzarella cheese", "type": "perishable" }
        ]
    },
    "directions": [
        { "text": "Place bread slices on a baking sheet, brush with olive oil and sprinkle on herbes de provence." },
        { "text": "Cook for 13 mins at 375° then set aside." },
        { "text": "Heat an iron skillet in the oven at 450°." },
        { "text": "Throw in mushrooms, thyme, rosemary and olive oil and roast in the oven for 14 mins." },
        { "text": "Season mushrooms with worcestershire, salt, pepper, and parsley." },
        { "text": "Spread on cheese on bread and then top with mushrooms." }
    ]
}
