{
    "title": "Pasta",
    "meta": "Makes 1 pound. Use '3' thickness on kitchen aid pasta maker.",
    "ingredients": [
        { "text": "1 cup semolina flour", "type": "imperishable" },
        { "text": "&frac12; cup white flour", "type": "imperishable" },
        { "text": "2 eggs", "type": "perishable" },
        { "text": "2 TBSP Olive oil", "type": "imperishable" },
        { "text": "1&frac12; tsp salt", "type": "spice" }
    ],
    "directions": [
        { "text": "Mix eggs, oil, and salt into middle of flour." },
        { "text": "Knead until a good consistency." },
        { "text": "Let stand at room temperature covered by a towel for 20 mins." },
        { "text": "Boil pasta for 3 mins" }
    ]
}
