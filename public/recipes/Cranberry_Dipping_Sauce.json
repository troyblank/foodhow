{
    "title": "Cranberry Dipping Sauce",
    "meta": "",
    "ingredients": [
        "1 pound of frozen cranberries",
        "2 cups freshly squeezed orange juice",
        "zest from one orange",
        "3 cups ginger beer",
        "2 TBSP maple syrup",
        "2 TBSP light brown sugar",
        "&frac12; tsp salt"
    ],
    "directions": [
        { "text": "Combine all ingredients and simmer for 45 mins." },
        { "text": "Let cool and smooth out in a food processor." }
    ]
}
