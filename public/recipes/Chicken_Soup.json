{
    "title": "Chicken Soup",
    "meta": "",
    "ingredients": [
        { "text": "[8 cups Chicken Stock](/recipe/Chicken_Stock)" },
        { "text": "2 green onions bunches", "type": "produce" },
        { "text": "3 carrots", "type": "produce" },
        { "text": "4 celery ribs", "type": "produce" },
        { "text": "1 rotisserie chicken", "type": "perishable" },
        { "text": "12oz package frozen egg noodles", "type": "frozen" },
        { "text": "2 TBSP butter", "type": "imperishable" },
        { "text": "&frac12; tsp salt", "type": "spice" },
        { "text": "&frac12; tsp pepper", "type": "spice" }
    ],
    "directions": [
        { "text": "Peel carrots and cut all vegetables into tiny bits." },
        { "text": "De-skin the chicken." },
        { "text": "Cook the noodles." },
        { "text": "Melt butter in a Dutch over over medium high heat and cook vegetables for 5 mins." },
        { "text": "Stir in broth and bring to boil." },
        { "text": "Cover under medium low heat for 15 mins." },
        { "text": "Stir in chicken, noodles, salt, and pepper. Then cover for 2 mins." }
    ]
}
