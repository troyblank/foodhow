{
    "title": "Grilled Tenderloin 2",
    "meta": "",
    "ingredients": [
        { "text": "3lb pork tenderloin", "type": "meat" },
        { "text": "2 oranges", "type": "produce" },
        { "text": "2 limes", "type": "produce" },
        { "text": "&frac12; cup dark rum", "type": "imperishable" },
        { "text": "1 medium onion thinly sliced", "type": "produce" },
        { "text": "6 garlic gloves minced", "type": "produce" },
        { "text": "1 bay leaf", "type": "spice" },
        { "text": "2 tsp oregano", "type": "spice" },
        { "text": "2 tsp cumin", "type": "spice" },
        { "text": "1 tsp salt", "type": "spice" },
        { "text": "&frac12; tsp black pepper;", "type": "spice" }
    ],
    "directions": [
        { "text": "Rinse and pat dry tenderloins. Juice oranges and limes." },
        { "text": "Combine juice and all other ingredients and place in a zip lock bag." },
        { "text": "refrigerate overnight." },
        { "text": "Pat tenderloins dry, cover in olive oil and grill for 8mins per side." },
        { "text": "Cover with foil, let rest for 5 mins, slice and serve." }
    ]
}
