{
    "title": "Venison Steaks",
    "meta": "",
    "ingredients": [
        "hickory wood chips",
        "4 venison steaks",
        "&frac12; cup canola oil",
        "1 TBSP oregano",
        "1 &frac12; tsp garlic powder",
        "1 &frac12; tsp onion powder",
        "1 &frac12; tsp seasoned salt",
        "1 &frac12; tsp basil",
        "1 &frac12; tsp parsley",
        "&frac12; tsp pepper",
        "&frac18; tsp thyme",
        "&frac18; tsp celery seed",
        "1 &frac12; tsp sugar"
    ],
    "directions": [
        { "text": "Combine oil, sugar and spices together in a small bowl." },
        { "text": "Put steaks in a small container and pour oil mixture on them, let chill for 6 hours." },
        { "text": "Remove steaks from marinade and smoke with hickory at 225°." },
        { "text": "Steaks are done when they reach 160°." }
    ]
}
