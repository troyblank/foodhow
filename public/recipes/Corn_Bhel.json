            { "text": "1 &frac12; tsp cumin", "type": "spice" },
            { "text": "&frac12; tsp cayenne powder", "type": "spice" },
            { "text": "1 tsp salt", "type": "spice" }
        ]
    },
    "directions": [
        { "text": "Process all chutney ingredients with &frac12; cup of water and set aside." },
        { "text": "Boil potatoes for 25 mins, and corn for 10 mins." },
        { "text": "Cook tortillas for 10 mins at 375Â° and then break into small pieces." },
