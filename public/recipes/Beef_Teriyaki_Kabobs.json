{
    "title": "Beef Teriyaki Kabobs",
    "meta": "3 hour marinate",
    "ingredients": [
        { "text": "4 small red onions", "type": "produce" },
        { "text": "1 red bell pepper", "type": "produce" },
        { "text": "1 yellow bell pepper", "type": "produce" },
        { "text": "8 oz mushrooms", "type": "produce" },
        { "text": "2 zucchini", "type": "produce" },
        { "text": "6 garlic gloves", "type": "produce" },
        { "text": "1\" ginger", "type": "produce" },
        { "text": "2 lbs beef flank steak", "type": "meat" },
        { "text": "1 cup soy sauce", "type": "imperishable" },
        { "text": "&frac12; cup rice vinegar", "type": "imperishable" },
        { "text": "&frac14; cup molasses", "type": "imperishable" },
        { "text": "&frac12; cup peanut oil", "type": "imperishable" },
        { "text": "1 TBSP toasted sesame oil", "type": "imperishable" },
        { "text": "1 tsp chili oil", "type": "imperishable" },
        { "text": "1 tsp mustard powder", "type": "spice" }
    ],
    "directions": [
        { "text": "Cut up onions, peppers, mushrooms, zucchini, and beef into bite size pieces." },
        { "text": "Mince garlic and ginger." },
        { "text": "In a sealable container mix all ingredients by shaking, refrigerate for 3 hours." },
        { "text": "Thread pieces onto skewers." },
        { "text": "Grill over direct heat for 3 minutes per side" }
    ]
}
