{
    "title": "Rigatoni Negatano",
    "meta": "",
    "ingredients": [
        { "text": "1.5 lb chicken", "type": "meat" },
        { "text": "2 lb zucchini", "type": "produce" },
        { "text": "1 lb mushrooms", "type": "produce" },
        { "text": "1 minced garlic", "type": "produce" },
        { "text": "1 oz fresh basil", "type": "produce" },
        { "text": "8 oz mozzarella", "type": "perishable" },
        { "text": "25 oz marinara sauce", "type": "imperishable" },
        { "text": "4 oz olive oil", "type": "imperishable" },
        { "text": "10 oz rigatoni pasta", "type": "imperishable" }
    ],
    "directions": [
        { "text": "Cut up chicken into bite sized pieces, cook and set aside." },
        { "text": "Cut up zucchini and mushrooms, cook and set aside." },
        { "text": "Cook pasta and set aside." },
        { "text": "Heat olive oil on mid heat in a big pot." },
        { "text": "Add chicken, noodles and marinara to pot and stir." },
        { "text": "Add zucchini, mushrooms, garlic, and basil to pot and stir." },
        { "text": "Add mozzarella slowly to pot and stir." },
        { "text": "Stir regularly for 15 mins." }
    ]
}