{
    "title": "Fauxgo Cheese Poofs",
    "meta": "",
    "ingredients": [
        { "text": "1 cup tapioca flour", "type": "imperishable" },
        { "text": "&frac12; cup whole milk", "type": "perishable" },
        { "text": "&frac13; cup butter", "type": "imperishable" },
        { "text": "&frac12; tsp salt", "type": "spice" },
        { "text": "1 oz shredded parmesan or cotija cheese", "type": "imperishable" },
        { "text": "1 egg", "type": "perishable" },
        { "text": "&frac12; TBSP olive oil", "type": "imperishable" },
        { "text": "1 tsp sugar", "type": "imperishable" },
        { "text": "butter cooking spray", "type": "imperishable" }
    ],
    "directions": [
        { "text": "Combine butter, salt and milk in a sauce pan and bring to low boil." },
        { "text": "As soon as it boils transfer to a mixer and set on low." },
        { "text": "Slowly add flour, then cheese, eggs, sugar and oil." },
        { "text": "Mix on medium for 2 minutes." },
        { "text": "Spray down 1 mini muffing tins with cooking spray and spoon in batter." },
        { "text": "Bake for 20 mins on 350Â°." }
    ]
}
