{
    "title": "Crumble Bars",
    "meta": "",
    "ingredients": {
        "Dough": [
            { "text": "1 cup quick oats", "type": "imperishable" },
            { "text": "&frac34; cup brown sugar", "type": "imperishable" },
            { "text": "2 cups flour", "type": "imperishable" },
            { "text": "3 tsp baking powder", "type": "imperishable" },
            { "text": "10 TBSP butter", "type": "perishable" },
            { "text": "&frac12; tsp salt", "type": "spice" }
        ],
        "filling": [
            { "text": "18 oz strawberry preserves", "type": "imperishable" }
        ]
    },
    "directions": [
        { "text": "Mix all dough ingredients with a blender cutting butter in slowly." },
        { "text": "Grease a 9 x 9 pan with butter and press half the dough into the bottom." },
        { "text": "Spread filling in the middle and gently top with rest of dough." },
        { "text": "Bake at 400Â° for 26 mins." }
    ]
}
