{
    "title": "Bourbon Chicken",
    "meta": "",
    "ingredients": [
        { "text": "1 garlic clove minced", "type": "produce" },
        { "text": "8 skinned chicken thighs", "type": "meat" },
        { "text": "&frac12; cup bourbon", "type": "imperishable" },
        { "text": "&frac14; cup brown sugar", "type": "imperishable" },
        { "text": "1 TBSP apple cider vinegar", "type": "imperishable" },
        { "text": "1 tsp Worcestershire sauce", "type": "imperishable" },
        { "text": "&frac34; tsp cornstarch", "type": "imperishable" },
        { "text": "2 TBSP vegetable oil", "type": "imperishable" },
        { "text": "&frac34; tsp Cajun seasoning", "type": "spice" },
        { "text": "2 pinches of salt", "type": "spice" },
        { "text": "2 pinches of pepper", "type": "spice" }
    ],
    "directions": [
        { "text": "Heat oil in large skillet over medium heat." },
        { "text": "Toss chicken with some salt and pepper." },
        { "text": "Cook chicken skin down for 18 mins." },
        { "text": "Flip chicken over and cook for another 8 mins, then set chicken aside." },
        { "text": "In a small bowl whisk the rest of the ingredients in along with another pinch of salt and pepper." },
        { "text": "Pour fat from skillet and cook sauce for 2 mins." },
        { "text": "Serve by drizzling sauce over chicken." }
    ]
}
