{
    "title": "Doctored Cinnamon Rolls",
    "meta": "",
    "ingredients": [
        { "text": "1 Pillsbury™ Grands!™ Cinnamon Rolls", "type": "imperishable" },
        { "text": "&frac34; cups heavy cream", "type": "perishable" },
        { "text": "2 TBSP brown sugar", "type": "imperishable" }
    ],
    "directions": [
        { "text": "Lay rolls out in a 9″ pie pan." },
        { "text": "Sprinkle brown sugar over the tops of the rolls." },
        { "text": "Pour heavy cream over the rolls." },
        { "text": "Cook at 350° for 25 mins." },
        { "text": "Spread in package icing over rolls." }
    ]
}
