{
    "title": "Cake",
    "meta": "",
    "ingredients": [
        { "text": "&frac34 cup butter flavored vegetable shortening", "type": "imperishable" },
        { "text": "2 &frac12; cups cups cake flour", "type": "imperishable" },
        { "text": "3 tsp baking powder", "type": "imperishable" },
        { "text": "&frac34; cup milk", "type": "perishable" },
        { "text": "8 eggs yolks", "type": "perishable" },
        { "text": "1 tsp vanilla extract", "type": "imperishable" },
        { "text": "1 &frac14; cups sugar", "type": "imperishable" },
        { "text": "&frac14; tsp salt", "type": "spice" }
    ],
    "directions": [
        { "text": "Heat oven to 350, and grease two cake pans with some extra shortening." },
        { "text": "Mix together shortening and sugar and start mixing on low." },
        { "text": "Then add flour, baking power and salt." },
        { "text": "Beat yolks with milk in a separate bowl." },
        { "text": "Slowly add egg mixture to the mixing bowl." },
        { "text": "Add the vanilla extract." },
        { "text": "Pour the mixture divided into the pans and cook for 25 mins." }
    ]
}
