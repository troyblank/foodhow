{
    "title": "Hazel Butt",
    "meta": "",
    "ingredients": [
        { "text": "2 cups raw hazel nuts", "type": "imperishable" },
        { "text": "3&frac14; oz Honey", "type": "imperishable" },
        { "text": "3&frac14; oz Vegetable oil", "type": "imperishable" },
        { "text": "2 tsp vanilla extract", "type": "imperishable" },
        { "text": "&frac12; cup maple syrup", "type": "imperishable" },
        { "text": "2 TBSP brown sugar", "type": "imperishable" },
        { "text": "2 tsp salt", "type": "spice" }
    ],
    "directions": [
        { "text": "Cook hazelnuts on medium high in a skillet for 5 mins." },
        { "text": "Remove most of hazelnut skins by rubbing inside of a towel." },
        { "text": "Mix all ingredients using a food processor until roughly smooth" }
    ]
}
