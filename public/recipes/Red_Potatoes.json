{
    "title": "Red Potatoes",
    "meta": "",
    "ingredients": [
        { "text": "1 lb small red potatoes", "type": "produce" },
        { "text": "1 TBSP olive oil", "type": "imperishable" },
        { "text": "2 TBSP butter", "type": "imperishable" },
        { "text": "1 TBSP fresh thyme (chopped)", "type": "produce" },
        { "text": "1 TBSP salt", "type": "spice" },
        { "text": "2 tsp pepper", "type": "spice" }
    ],
    "directions": [
        { "text": "Cut potatoes in fourths and boil for 12 mins." },
        { "text": "Add oil and butter to a pan over medium high heat and brown potatoes in pan for 8 mins." },
        { "text": "Mix in thyme, salt, and pepper." }
    ]
}
