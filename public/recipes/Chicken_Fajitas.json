{
    "title": "Chicken Fajitas",
    "meta": "",
    "ingredients": [
        { "text": "6 chicken breasts", "type": "meat" },
        { "text": "&frac12; cup tequila", "type": "imperishable" },
        { "text": "&frac12; cup olive oil", "type": "imperishable" },
        { "text": "12 large tortillas", "type": "imperishable" },
        { "text": "4 limes", "type": "produce" },
        { "text": "2 green peppers", "type": "produce" },
        { "text": "2 red peppers", "type": "produce" },
        { "text": "1 large yellow onion", "type": "produce" },
        { "text": "1 TB chili powder", "type": "spice" },
        { "text": "1 TB cumin", "type": "spice" },
        { "text": "1 TB garlic powder", "type": "spice" },
        { "text": "1 tsp salt", "type": "spice" },
        { "text": "1 tsp pepper", "type": "spice" }
    ],
    "directions": [
        { "text": "Combine tequila, juice from limes, &frac14; cup olive oil and all spices." },
        { "text": "Add chicken and tequila mixture to a Ziploc bag and marinate overnight in refrigeration." },
        { "text": "Grill chicken breasts for about 7 minutes each side, let rest and cut into small strips." },
        { "text": "With 1 TB olive oil fry sliced vegetables in a skillet over medium high heat for 4 mins." }
    ]
}
