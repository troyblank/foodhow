{
    "title": "Cinnamon Squash Bread",
    "meta": "",
    "ingredients": [
        { "text": "1 butternut squash", "type": "produce" },
        { "text": "2 eggs", "type": "imperishable" },
        { "text": "1 cup vegetable oil", "type": "imperishable" },
        { "text": "2 cups sugar", "type": "imperishable" },
        { "text": "3 cups flour", "type": "imperishable" },
        { "text": "&frac12; cup cashews", "type": "imperishable" },
        { "text": "2 tsp vanilla extract", "type": "imperishable" },
        { "text": "3 tsp baking powder", "type": "imperishable" },
        { "text": "1 tsp cinnamon", "type": "spice" },
        { "text": "1 tsp salt", "type": "spice" }
    ],
    "directions": [
        { "text": "Grate squash down to 2 cups worth." },
        { "text": "Blend eggs while adding oil, sugar, squash and vanilla until smooth." },
        { "text": "Shift together the flour, baking powder, cinnamon and salt." },
        { "text": "Blend the flour mixture into the wet mixture." },
        { "text": "Chop nuts up and fold into mixture." },
        { "text": "Grease two 8.5 x 4.5\" loaf pans and pour mixture evenly between the two." },
        { "text": "Bake at 325 Â° for 55 minutes." }
    ]
}
