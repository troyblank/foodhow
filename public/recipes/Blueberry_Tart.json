{
    "title": "Blueberry Tart",
    "meta": "",
    "ingredients": {
        "Dough": [
            { "text": "2 cups flour", "type": "imperishable" },
            { "text": "3 tsp baking powder", "type": "imperishable" },
            { "text": "6 TBSP butter", "type": "perishable" },
            { "text": "&frac14; cup sugar", "type": "imperishable" },
            { "text": "&frac12; tsp salt", "type": "spice" }
        ],
        "Topping": [
            { "text": "21 ox blueberry pie filling", "type": "imperishable" },
            { "text": "&frac12; cup dried cranberries", "type": "imperishable" },
            { "text": "1 tsp orange zest", "type": "produce" }
        ]
    },
    "directions": [
        { "text": "Mix all dough ingredients while slowly adding small bits of the butter one at a time." },
        { "text": "When mix is crumbly press firmly on bottom of a 9″ spring form pan." },
        { "text": "Bake at 375° for 13.5 mins then let cool for at least 30 mins." },
        { "text": "Mix topping ingredients then spread over crust." }
    ]
}
