        { "text": "2 tsp vanilla extract", "type": "imperishable" },
        { "text": "1 &frac12; cup light brown sugar", "type": "imperishable" },
        { "text": "&frac12; tsp cinnamon", "type": "spice" },
        { "text": "1 &frac12; tsp salt", "type": "spice" }
    ],
    "directions": [
