            { "text": "2 cups cilantro leaves", "type": "produce" },
            { "text": "1 serranos pepper", "type": "produce" },
            { "text": "&frac12; green pepper", "type": "produce" },
            { "text": "1 tsp ginger", "type": "produce" },
            { "text": "&frac14; cup lemon juice", "type": "produce" },
            { "text": "1 tsp sugar", "type": "imperishable" },
            { "text": "&frac12; tsp salt", "type": "spice" }
        ],
        "Bhel": [
            { "text": "&frac34; cup sev", "type": "uncommon" },
            { "text": "2 cups potatoes (diced)", "type": "produce" },
            { "text": "&frac12; cup cilantro (chopped}", "type": "produce" },
            { "text": "&frac13; red onion (diced)", "type": "produce" },
            { "text": "&frac14; cup lime juice", "type": "produce" },
