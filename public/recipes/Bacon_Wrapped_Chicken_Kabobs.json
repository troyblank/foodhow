{
    "title": "Bacon Wrapped Chicken Kabobs",
    "meta": "",
    "ingredients": [
        { "text": "1 lb mushrooms", "type": "produce" },
        { "text": "1 large red onion", "type": "produce" },
        { "text": "1 pineapple", "type": "produce" },
        { "text": "1 red pepper", "type": "produce" },
        { "text": "2 chicken breasts", "type": "meat" },
        { "text": "1 lb bacon", "type": "meat" },
        { "text": "&frac12; cup orange marmalade", "type": "imperishable" },
        { "text": "&frac12; soy sauce", "type": "imperishable" },
        { "text": "2 TB honey", "type": "imperishable" },
        { "text": "1 tsp ginger powder", "type": "spice" },
        { "text": "1 garlic clove", "type": "produce" }
    ],
    "directions": [
        { "text": "Combine marmalade, soy sauce, honey, ginger and diced garlic in a saucepan." },
        { "text": "Heat over medium until boiling stirring frequently until half it's volume, set aside." },
        { "text": "Cut pineapple, veggies, and chicken up in bite size pieces." },
        { "text": "Thread each piece through a skewer, wrapping chicken with bacon." },
        { "text": "Baste kabobs and grill on each side for about 8 minutes." }
    ]
}