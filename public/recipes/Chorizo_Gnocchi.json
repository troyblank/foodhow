{
    "title": "Chorizo Gnocchi",
    "ingredients": [
        "[3 cups Gnocchi](/recipe/gnocchi)",
        "1 lb chorizo",
        "2 red bell peppers (diced)",
        "4 cups spinach",
        "1 yellow onion (sliced)",
        "6 garlic cloves (minced)",
        "2 TBSP olive oil",
        "2 tsp italian seasoning",
        "1 tsp salt"
    ],
    "directions": [
        { "text": "sauté chorizo with oil, onion, pepper, and gnocchi for 4 mins on high heat." },
        { "text": "Mix in spinach, garlic, and spices and sauté for 2 additional mins." }
    ]
}
