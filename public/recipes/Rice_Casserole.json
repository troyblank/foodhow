{
    "title": "Rice Casserole",
    "meta": "Grandma's",
    "ingredients": [
        { "text": "16 oz mushrooms", "type": "produce" },
        { "text": "1 yellow onion", "type": "produce" },
        { "text": "11 TBSP butter", "type": "imperishable" },
        { "text": "21 oz consommé", "type": "imperishable" },
        { "text": "1 TBSP soy sauce", "type": "imperishable" },
        { "text": "1 &frac14; cups rice", "type": "imperishable" }
    ],
    "directions": [
        { "text": "Dice onion and saute with 1 TBSP butter." },
        { "text": "Dice mushrooms and saute with 2 TBSP butter." },
        { "text": "Put everything in a covered casserole dish." },
        { "text": "Cook at 325° for 1 hour. stirring mid way through." }
    ]
}
